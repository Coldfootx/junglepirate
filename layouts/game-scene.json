      "inlineCode": [
        "const pc_name_texts = runtimeScene.getObjects(\"pc_name_text\");",
        "const shamans = runtimeScene.getObjects(\"Shaman\");",
        "const necromancers = runtimeScene.getObjects(\"Necromancer\");",
        "",
        "pc_name_texts.forEach(pc_name_textz => {",
        "    necromancers.forEach(necromancerz => {",
        "        if (pc_name_textz.id == necromancerz.id) {",
        "            pc_name_textz.setX(necromancerz.getX()+necromancerz.getWidth()/2-pc_name_textz.getWidth()/2);",
        "            pc_name_textz.setY(necromancerz.getY()-pc_name_textz.getHeight());",
        "            runtimeScene.getGame().getVariables().get('text_buffer').setString(runtimeScene.getGame().getVariables().get('text_buffer').getAsString()+ \", \" + toString(necromancerz.id));",
        "        };",
        "    });",
        "});",
        "",
        "pc_name_texts.forEach(pc_name_textz => {",
        "    shamans.forEach(shamanz => {",
        "        if (pc_name_textz.id == shamanz.id) {",
        "            pc_name_textz.setX(shamanz.getX()+shamanz.getWidth()/2-pc_name_textz.getWidth()/2);",
        "            pc_name_textz.setY(shamanz.getY()-pc_name_textz.getHeight());",
        "            runtimeScene.getGame().getVariables().get('text_buffer').setString(runtimeScene.getGame().getVariables().get('text_buffer').getAsString()+ \", \" + toString(shamanz.id));",
        "        };",
        "    });",
        "});"
